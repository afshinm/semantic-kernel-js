{
  "name": "@semantic-kernel/abstractions",
  "description": "Semantic Kernel Abstractions",
  "version": "0.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://kerneljs.com",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test:eslint": "eslint \"src/**/*.ts*\"",
    "test:prettier": "prettier --check \"src/**/*.ts*\"",
    "test:jest": "jest",
    "test": "run-p test:*"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@semantic-kernel/common": "*",
    "@semantic-kernel/tsconfig": "*",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.13.9",
    "eslint": "^9.9.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  },
  "dependencies": {
    "@semantic-kernel/ai": "*",
    "fast-xml-parser": "^5.2.5",
    "handlebars": "^4.7.8"
  }
}
