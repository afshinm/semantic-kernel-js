{
  "name": "@semantic-kernel/react",
  "description": "Semantic Kernel React",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://kerneljs.com",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test:eslint": "eslint \"src/**/*.ts*\"",
    "test:prettier": "prettier --check \"src/**/*.ts*\"",
    "test:jest": "jest",
    "test": "run-p test:*"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@semantic-kernel/abstractions": "*",
    "@semantic-kernel/openai": "*"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@semantic-kernel/tsconfig": "*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
