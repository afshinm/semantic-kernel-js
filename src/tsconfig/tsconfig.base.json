{
  "compilerOptions": {
    "lib": ["es2018"],
    "target": "es2018",
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "strictBindCallApply": false,
    "useUnknownInCatchVariables": false,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnusedLabels": false,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "preserveConstEnums": true,
    "types": ["node"]
  }
}
